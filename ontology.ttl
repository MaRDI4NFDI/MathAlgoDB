###  https://mardi4nfdi.de/mathalgodb/0.1#algorithm
:algorithm rdf:type owl:Class ;
           rdfs:comment "The main building blocks of the graph are algorithms. Here you can take a look at all algorithms documented in the graph." ;
           rdfs:label "Algorithm" ;
           owl:cardinality "10"^^xsd:nonNegativeInteger .


###  https://mardi4nfdi.de/mathalgodb/0.1#all_objects
:all_objects rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:unionOf ( :algorithm
                                                 :benchmark
                                                 :problem
                                                 :publication
                                                 :software
                                               )
                                 ] ;
             rdfs:label "All allgodata objects" .


###  https://mardi4nfdi.de/mathalgodb/0.1#benchmark
:benchmark rdf:type owl:Class ;
           rdfs:comment "For testing, evaluating and comparing numerical algorithms, so called benchmark problems are often useful." ;
           rdfs:label "Benchmark" ;
           owl:cardinality "40"^^xsd:nonNegativeInteger .


###  https://mardi4nfdi.de/mathalgodb/0.1#identifiable
:identifiable rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:unionOf ( :benchmark
                                                  :publication
                                                  :software
                                                )
                                  ] ;
              rdfs:label "Identifiable" .


###  https://mardi4nfdi.de/mathalgodb/0.1#problem
:problem rdf:type owl:Class ;
         rdfs:comment "When interested in algorithms solving a certain problem at hand, we here provide an overview of all problems defined in the graph." ;
         rdfs:label "Algorithmic Task" ;
         owl:cardinality "30"^^xsd:nonNegativeInteger ;
         <http://www.w3.org/2004/02/skos/core#definition> "An engineering challenge that includes a mathematical part" .


###  https://mardi4nfdi.de/mathalgodb/0.1#publication
:publication rdf:type owl:Class ;
             rdfs:comment "To find theoretical or numerical results on the performance of algorithms, take a look at the related publications in this list." ;
             rdfs:label "Publication" ;
             owl:cardinality "100"^^xsd:nonNegativeInteger .


###  https://mardi4nfdi.de/mathalgodb/0.1#realization
:realization rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:unionOf ( :benchmark
                                                 :software
                                               )
                                 ] ;
             rdfs:label "Realization" ;
             <http://www.w3.org/2004/02/skos/core#definition> """
    Union of all MathAlgoDB-Classes that *may* have a unique identifier.
    """ .


